<?xml version="1.0" encoding="utf-8"?><Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>IDB_NotifyRejectionToSubmitter</fullName>
        <description>NotifyRejectionToSubmitter</description>
        <protected>false</protected>
        <recipients>
            <field>IDB_ApprovalSubmitter__c</field>
            <type>email</type>
        </recipients>
        <senderType>CurrentUser</senderType>
        <template>IDB_Invest_Notification_Templates/IDB_Rejected_Checklist_Item_Submitter_Email</template>
    </alerts>
    <fieldUpdates>
        <fullName>IDB_EmailUpdate</fullName>
        <description>This field is used to update the Email</description>
        <field>IDB_ApprovalSubmitter__c</field>
        <formula>$User.Email</formula>
        <name>EmailUpdate</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>IDB_UpdateLastName</fullName>
        <field>IDB_ApprovalSubmitter_LastName__c</field>
        <formula>$User.LastName</formula>
        <name>UpdateLastName</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>IDB_UpdateToApproved</fullName>
        <field>Status__c</field>
        <literalValue>Approved</literalValue>
        <name>UpdateToApproved</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>IDB_UpdateToRejected</fullName>
        <field>Status__c</field>
        <literalValue>Rejected</literalValue>
        <name>UpdateToRejected</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>IDB_UpdateToSubmitted</fullName>
        <description>This field is used to update the status to "Submitted"</description>
        <field>Status__c</field>
        <literalValue>Submitted</literalValue>
        <name>UpdateToSubmitted</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
</Workflow>
